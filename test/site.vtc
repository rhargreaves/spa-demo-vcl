test "response header returned"

server s1 {
	rxreq
	expect req.url == "/counter"
	txresp -body "{\"value\":12000}"
	txreq
	expect req.url == "/counter"
	txresp -body "{\"value\":12005}"
} -start

varnish v1 -vcl+backend {
	# INSERT_VCL_HERE
} -start

client c1 {
	txreq -url "/counter"
	rxresp
	expect resp.body == "{\"value\":12000}"
	txreq -url "/counter"
	rxresp
	expect resp.body == "{\"value\":12005}"
}

varnish v1 -expect cache_miss == 0
varnish v1 -expect cache_hit == 0

client c1 -run

varnish v1 -expect cache_miss == 1
varnish v1 -expect cache_hit == 0
